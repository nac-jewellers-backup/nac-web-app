{
  "name": "nac-ecom-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.2.0",
    "@material-ui/icons": "4.2.1",
    "apollo-boost": "^0.4.4",
    "apollo-fetch": "^0.7.0",
    "classnames": "2.2.6",
    "craco-alias": "^1.0.4",
    "graphql": "^14.5.7",
    "moment": "^2.24.0",
    "react": "16.8.6",
    "react-apollo": "^3.1.1",
    "react-bootstrap": "1.0.0-beta.9",
    "react-dom": "16.8.6",
    "react-js-pagination": "3.0.2",
    "react-lazy-load-image-component": "^1.3.2",
    "react-multi-carousel": "2.1.1",
    "react-photo-gallery": "8.0.0",
    "react-pose": "4.0.8",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-slick": "0.24.0",
    "react-star-ratings": "2.3.0",
    "react-swipeable-views": "0.13.3",
    "react-swipeable-views-utils": "0.13.3",
    "semantic-ui-react": "0.87.3",
    "slick-carousel": "1.8.1",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "generate-build-version": "node generate-build-version",
    "start": "craco  start",
    "build": "npm run generate-build-version && craco build",
    "eject": "react-scripts eject",
    "test": "craco  test",
    "serve": "npm run build && serve -S build",
    "lint": "eslint src/*.js",
    "hint": "hint http://localhost:8080",
    "generate-report": "lighthouse --chrome-flags=\"--headless --no-sandbox\" http://localhost:8080 --output html --output-path ./report.html",
    "pm2-serve": "pm2 delete nac-ecom || pm2 serve --spa build --name nac-ecom",
    "pm2-rm": "pm2 delete nac-ecom",
    "sb": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "5.2.4",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/react": "5.1.10",
    "hint": "^5.2.0",
    "nodemon": "1.19.1",
    "pm2": "3.5.1",
    "serve": "11.0.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.20",
    "storybook-react-router": "^1.0.5"
  },
  "cracoConfig": "craco-config.js"
}
